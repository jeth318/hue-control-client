{
  "name": "home-control-client",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "simple-git-hooks": {
    "pre-push": "./deploy.sh"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "postinstall": "rm -rf .git/hooks/ && npx simple-git-hooks"
  },
  "dependencies": {
    "axios": "0.26.1",
    "core-js": "3.21.1",
    "cron": "1.8.2",
    "date-fns": "^2.28.0",
    "node-hue-api": "5.0.0-beta.13",
    "node-schedule": "2.1.0",
    "ramda": "0.28.0",
    "rtsp-client": "^1.4.5",
    "rtsp-relay": "^1.6.1",
    "scheduler": "0.20.2",
    "vue": "^2.0.0",
    "vuetify": "^2.6.5",
    "vuex": "^3.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@vue/cli-plugin-babel": "5.0.4",
    "@vue/cli-plugin-eslint": "5.0.4",
    "@vue/cli-plugin-vuex": "5.0.4",
    "@vue/cli-service": "5.0.4",
    "eslint": "8.11.0",
    "eslint-plugin-vue": "8.5.0",
    "fs": "^0.0.1-security",
    "git-branch-is": "^4.0.0",
    "sass": "1.49.9",
    "sass-loader": "12.6.0",
    "simple-git-hooks": "^2.7.0",
    "vue-cli-plugin-vuetify": "2.4.8",
    "vue-template-compiler": "2.6.14",
    "vuetify-loader": "1.7.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
